   "source": [
    "# Filtering the data for the instrumentation 'track', 'Electronics > 70', and Hometown = 'Luzon'\n",
    "instru_df = df[(df['Track'] == 'Instrumentation') \n",
    "            &(df['Hometown'] == 'Luzon')\n",
    "            &(df['Electronics'] > 70)][['Name', 'GEAS', 'Electronics']] \n",
    "\n",
    "#Calling the filtered df \n",
    "instru_df"
   ]
